# We want fine grained libraries, so use the new code to build the
# floating point emulation libraries.
FPBIT = fp-bit.c
DPBIT = dp-bit.c

$(gcc_objdir)/dp-bit.c: $(gcc_srcdir)/config/fp-bit.c
	cat $<			> $@

$(gcc_objdir)/fp-bit.c: $(gcc_srcdir)/config/fp-bit.c
	echo '#define FLOAT'	> $@
	cat $<			>> $@
